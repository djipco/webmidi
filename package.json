{
  "name": "webmidi",
  "version": "3.0.0-alpha.0",
  "description": "WebMidi.js makes it easy to talk to MIDI instruments from a browser or from Node.js. It simplifies the control of external or virtual MIDI instruments with functions such as playNote(), setPitchBend(), sendControlChange(), etc. It also allows reacting to incoming MIDI messages by adding listeners for user-friendly events such as 'noteon', 'pitchbend', 'songposition', etc.",
  "main": "dist/webmidi.cjs.min.js",
  "module": "dist/webmidi.esm.min.js",
  "author": {
    "name": "Jean-Philippe Côté",
    "email": "webmidi@djip.co",
    "url": "https://djip.co"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djipco/webmidi.git"
  },
  "bugs": {
    "url": "https://github.com/djipco/webmidi/issues"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "midi",
    "message",
    "web",
    "browser",
    "front-end",
    "web midi api",
    "music",
    "djipco",
    "music",
    "protocol",
    "communication",
    "channel",
    "node",
    "instrument",
    "device",
    "note"
  ],
  "homepage": "https://webmidijs.org",
  "license": "MIT",
  "webmidi": {
    "name": "WebMidi.js",
    "tagline": "A JavaScript library to kickstart your MIDI projects"
  },
  "engines": {
    "node": ">=8.5"
  },
  "scripts": {
    "build:all": "npm run lint && npm run build:cjs && npm run build:esm && npm run build:iife",
    "build:cjs": "node scripts/build/build.js -t cjs",
    "build:esm": "node scripts/build/build.js -t esm",
    "build:iife": "node scripts/build/build.js -t iife",
    "coverage:text": "nyc --reporter=text npm run test:all",
    "coverage:html": "nyc --reporter=html npm run test:all",
    "docs:generate": "node scripts/documentation/generate.js",
    "docs:publish": "node scripts/documentation/publish.js",
    "docs:generate-and-publish": "npm run docs:generate && npm run docs:publish",
    "lint": "eslint ./src/*.js",
    "release:next": "npm version prerelease && npm run build:all && node scripts/release/release.js && npm publish --tag next",
    "release:latest": "npm version patch && npm run build:all && node scripts/release/release.js && npm publish",
    "test:all": "mocha test/suites/*",
    "test:Input": "mocha test/suites/Input.test.js",
    "test:InputChannel": "mocha test/suites/InputChannel.test.js",
    "test:Note": "mocha test/suites/Note.test.js",
    "test:Output": "mocha test/suites/Output.test.js",
    "test:OutputChannel": "mocha test/suites/OutputChannel.test.js",
    "test:WebMidi": "mocha test/suites/WebMidi.test.js",
    "types": "tsc --strict --noEmit ./webmidi.d.ts"
  },
  "types": "webmidi.d.ts",
  "dependencies": {
    "djipevents": "^1.0.0-beta.4"
  },
  "optionalDependencies": {
    "jzz": "^1.0.4"
  },
  "devDependencies": {
    "@alexbinary/rimraf": "^1.0.2",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "foodoc": "0.0.9",
    "ftp-deploy": "^2.3.7",
    "gh-pages": "^2.2.0",
    "jsdoc": "^3.6.3",
    "markdown-it": "^10.0.0",
    "midi": "^1.0.0",
    "minimist": "^1.2.5",
    "mocha": "^7.1.1",
    "moment": "^2.25.3",
    "nyc": "^15.0.1",
    "rollup": "^2.3.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-license": "^2.0.0",
    "rollup-plugin-strip-code": "^0.2.7",
    "rollup-plugin-terser": "^5.3.0",
    "simple-git": "^2.4.0",
    "sinon": "^9.0.1",
    "sinon-browser-only": "^1.12.1",
    "system-commands": "^1.1.7",
    "typescript": "^3.8.3"
  }
}
